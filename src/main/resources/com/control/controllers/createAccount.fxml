<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="createAccountBox" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.control.controllers.CreateController">
   <Label fx:id="createText" text="Create Account" />
   <TextField fx:id="username" promptText="Username" />
   <PasswordField fx:id="password" promptText="Password" />
   <PasswordField fx:id="passwordConfirm" promptText="Confirm Password" />
   <TextField fx:id="firstName" promptText="First Name" />
   <TextField fx:id="lastName" promptText="Last Name" />
   <HBox>
      <Button onAction="#onBackClick" text="Back" />
      <Region HBox.hgrow="ALWAYS" />
      <Button onAction="#onCreateClick" text="Create" />
   </HBox>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
