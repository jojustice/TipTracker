<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<VBox maxHeight="-Infinity" spacing="20.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.control.controllers.ExpensesController">
    <children>
        <ComboBox fx:id = "expenseChoice" prefWidth="150.0" promptText="New Expense" onAction="#onSelectedExpense"/>
        <TextField fx:id = "expenseName" promptText="Label" />
        <TextField fx:id = "expenseAmount" promptText="Amount" />
        <Button fx:id = "addExpense" onAction="#onSubmitExpenseButtonClick"  mnemonicParsing="false" text="Submit" />
        <Button fx:id = "removeExpense" onAction="#onRemoveExpenseButtonClick" mnemonicParsing="false" text="Remove Expense" />
    </children>
</VBox>
